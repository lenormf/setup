# Configuration for domain {{ item.domain }}

server {
       listen 80;
       listen [::]:80;

       access_log /srv/http/{{ item.domain }}/logs/reverse-access.log;
       error_log /srv/http/{{ item.domain }}/logs/reverse-error.log;

       location / {
       		proxy_pass http://127.0.0.1:{{ item.local_port }};
	}
}
